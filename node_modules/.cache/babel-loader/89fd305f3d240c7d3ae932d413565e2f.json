{"ast":null,"code":"export default {\n  name: 'AppDescription',\n\n  data() {\n    return {\n      img: '',\n      facts: [],\n      cats: false,\n      fishes: false,\n      fishCode: 0\n    };\n  },\n\n  methods: {\n    goToGatti() {\n      this.axios.post('/description/catintheface').then(response => {\n        this.fishes = false;\n        this.img = \"data:image/jpeg;base64,\" + response.data;\n        this.cats = true;\n      }).catch(error => {\n        console.log(error);\n      });\n      this.axios.post('/description/catsfacts').then(response => {\n        for (let i in response.data) this.facts.push(response.data[i]);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    goToPesci() {\n      this.axios.post('/description/fishonthetable').then(response => {\n        this.cats = false;\n        this.img = response.data.image;\n        this.fishCode = response.data.number;\n        this.fishes = true;\n        this.FishInfo(this.fishCode);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    FishInfo(n) {\n      this.axios.post('/description/fishinfo', {\n        headers: this.axios.defaults.headers,\n        num: n\n      }).then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    goToRoditori() {\n      this.axios.post('/Roditori').then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    goToCani() {\n      this.axios.post('/Cani').then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    goToUccelli() {\n      this.axios.post('/Uccelli').then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwBA,eAAe;EACbA,IAAI,EAAE,gBADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MACLC,GAAG,EAAE