r(let i in response.data) this.facts.push(response.data[i])\r\n        })\r\n        .catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    goToPesci(){\r\n      this.axios.post('/description/fishonthetable')\r\n      .then((response) => {\r\n        this.cats = false\r\n        this.img = response.data.image\r\n        this.fishCode = response.data.number\r\n        this.fishes = true\r\n        this.FishInfo(this.fishCode)\r\n      })\r\n      .catch((error) => {\r\n        console.log(error)\r\n      })\r\n    },\r\n\r\n    FishInfo(n){\r\n      this.axios.post('/description/fishinfo', {\r\n        headers: this.axios.defaults.headers,\r\n        num: n\r\n      })\r\n      .then((response) => {\r\n        console.log(response)\r\n      })\r\n      .catch((error) => {\r\n        console.log(error)\r\n      })\r\n    },\r\n\r\n    goToRoditori(){\r\n        this.axios.post('/Roditori')\r\n          .then((response) => {\r\n            console.log(response)\r\n          })\r\n          .catch((error) => {\r\n            console.log(error)\r\n          })\r\n    },\r\n\r\n    goToCani(){\r\n        this.axios.post('/Cani')\r\n          .then((response) => {\r\n            console.log(response)\r\n          })\r\n          .catch((error) => {\r\n            console.log(error)\r\n          })\r\n    },\r\n    \r\n    goToUccelli(){\r\n        this.axios.post('/Uccelli')\r\n          .then((response) => {\r\n            console.log(response)\r\n          })\r\n          .catch((error) => {\r\n            console.log(error)\r\n          })\r\n    }\r\n  }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}